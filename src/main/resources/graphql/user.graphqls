type User{
    long id : Long
    name : String
    phone : String
    email : String
    address : String
}

type Query{
	save(user : User) : [User]
    getUserById(id : ID) : [User]
    getUserByEmail(email : String) : [User]
    getAllUsers : [User]
}

type Mutation{
	deleteUser(User user) : [User];
}



